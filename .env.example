## Copy this file to .env and fill in the real values

# Supabase project URL
SUPABASE_URL=your-supabase-url

# Backend uses service role for admin ops (Auth management, profile sync)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Public anon key used to create RLS-bound clients per request
SUPABASE_ANON_KEY=your-anon-key

# JWT secret for our API (not Supabase)
JWT_SECRET=change-me
